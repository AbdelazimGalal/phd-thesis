@Article{	  EnhancedSuffixArrays,
  author	= {Mohamed Ibrahim Abouelhoda and Stefan Kurtz and Enno
		  Ohlebusch},
  title		= {Replacing suffix trees with enhanced suffix arrays},
  journal	= {Journal of Discrete Algorithms},
  year		= {2004},
  volume	= {2},
  number	= {1},
  pages		= {53 - 86},
  issn		= {1570-8667},
  note		= {The 9th International Symposium on String Processing and
		  Information Retrieval},
  abstract	= {The suffix tree is one of the most important data
		  structures in string processing and comparative genomics.
		  However, the space consumption of the suffix tree is a
		  bottleneck in large scale applications such as genome
		  analysis. In this article, we will overcome this obstacle.
		  We will show how every algorithm that uses a suffix tree as
		  data structure can systematically be replaced with an
		  algorithm that uses an enhanced suffix array and solves the
		  same problem in the same time complexity. The generic name
		  enhanced suffix array stands for data structures consisting
		  of the suffix array and additional tables. Our new
		  algorithms are not only more space efficient than previous
		  ones, but they are also faster and easier to implement.},
  doi		= {https://doi.org/10.1016/S1570-8667(03)00065-0},
  keywords	= {Suffix array, Suffix tree, Repeat analysis, Genome
		  comparison, Pattern matching},
  url		= {http://www.sciencedirect.com/science/article/pii/S1570866703000650}
}

