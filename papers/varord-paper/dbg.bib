@Article{assemblathon2,
author={Bradnam, Keith R. and others},
title="Assemblathon 2: evaluating de novo methods of genome assembly in three vertebrate species",
journal="GigaScience",
year="2013",
volume="2",
number="1",
pages="1--31",
abstract="The process of generating raw genome sequence data continues to become cheaper, faster, and more accurate. However, assembly of such data into high-quality, finished genome sequences remains challenging. Many genome assembly tools are available, but they differ greatly in terms of their performance (speed, scalability, hardware requirements, acceptance of newer read technologies) and in their final output (composition of assembled sequence). More importantly, it remains largely unclear how to best assess the quality of assembled genome sequences. The Assemblathon competitions are intended to assess current state-of-the-art methods in genome assembly.",
issn="2047-217X",
doi="10.1186/2047-217X-2-10",
url="http://dx.doi.org/10.1186/2047-217X-2-10"
}

